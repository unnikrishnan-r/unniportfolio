    "aos": "^3.0.0-beta.6",
    "dotenv": "^16.4.1",
    "env-yaml": "^0.1.2",
    "express": "^4.19.2",
    "if-env": "^1.0.4",
    "jest": "^29.7.0",
    "mysql2": "^3.9.4",
    "npm-check": "^6.0.1",
    "sequelize": "^6.35.2"
  },
